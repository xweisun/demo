<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sun.mapper.EmployeeMapper">



    <select id="selectemps" resultType="com.sun.entity.Employee">

        SELECT last_name AS lastName,email as email,gender as gender,age as age FROM tbl_employee WHERE gender = 1

    </select>


</mapper>